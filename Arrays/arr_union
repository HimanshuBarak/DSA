
//program to find union of two arrays in c++


int doUnion(int a[], int n, int b[], int m)  {
        //code here
        unordered_set <int> uion;

        for(int i=0;i<n;i++)
         uion.insert(a[i]);

        for(int i=0;i<m;i++)
         uion.insert(b[i]); 

         return uion.size();
    }
